<?xml version="1.0" encoding="utf-8" ?>
<SearchPortRequestHistory>
    <MainSQL>
      <![CDATA[SELECT 
		CSTID
		, CSTSTAT AS 상태
		, REQ_STAT_CODE
		, EQPTID
		, PORT_ID
		, REQ_TRGT_EQPTID
		, TRGT_EQPT_MOUNT_PSTN_ID AS UWD_ID
		, REQ_TYPE_CODE AS 요청
		, INSDTTM
		, UPDDTTM
		, PRCS_TYPE_CODE
		, RTD_RULE_ID
		, REQ_SEQS
		, ERR_CODE
		, RTD_EXEC_LOG_CNTT
		, INSUSER
		, REQ_SEQNO
		FROM TB_MHS_TRF_REQ_HIST
		WHERE INSDTTM BETWEEN TO_DATE(@StartTime, 'YYYY-MM-DD HH24:MI:SS') AND TO_DATE(@EndTime, 'YYYY-MM-DD HH24:MI:SS')
		AND ROWNUM <= 1000
		+Option01
		+Option02    -- 바인딩 안됨
		+Option03
		+Option04
		+Option05
		ORDER BY INSDTTM, REQ_SEQNO DESC]]>
	</MainSQL>
    <Option>
        <Option01 type="IF" condition="not_equal" key="DURABLE_ID" dataType="string" default="">
            AND CSTID IN (@DURABLE_ID)
        </Option01>
		<Option02 type="IF" condition="not_equal" key="EQUIPMENT_ID" dataType="string" default="">
            AND EQPTID LIKE @EQUIPMENT_ID
        </Option02>
        <Option03 type="IF" condition="not_equal" key="PORT_ID" dataType="string" default="">
            AND PORT_ID LIKE @PORT_ID
        </Option03>
        <Option04 type="IF" condition="not_equal" key="DURABLE_STATUS_CODE" dataType="" default="">
            AND CSTSTAT = @DURABLE_STATUS_CODE
        </Option04>
		<Option05 type="IF" condition="not_equal" key="RTD_RULE_ID" dataType="string" default="">
            AND RTD_RULE_ID LIKE @RTD_RULE_ID
        </Option05>
        <Option06 type="VAR" condition="" key="StartTime" dataType="DateTime" default=""/>
        <Option07 type="VAR" condition="" key="EndTime" dataType="DateTime" default=""/>
    </Option>
    <AdditionalVariable>
    </AdditionalVariable>
    <Coloring>
        <REQ_STAT_CODE Red="255" Green="155" Blue="155" IsColoringColumn="false" IsTimeCompare="false">
            <NO_DATA Red="255" Green="164" Blue="201" IsColoringRow="true"/>
        </REQ_STAT_CODE>
    </Coloring>
    <EventValues/>
</SearchPortRequestHistory>