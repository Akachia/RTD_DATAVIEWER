<?xml version="1.0" encoding="utf-8" ?>
<SearchCarrierEventAbbreviatedRecord>
    <MainSQL>
        SELECT
        CSTID
        , CSTSTAT AS 상태
        , LOTID
        , PROCID
        , EQPTID
        , PORT_ID
        , RACK_ID
        , EVENT_CODE
        , EVENT_TYPE_CODE
        , TRF_END_CODE
        , FINL_TO_EQPTID
        , FINL_TO_PORT_ID
        , INSUSER
        , INSDTTM
        , UPDDTTM
        FROM TB_MHS_TRF_EVENT_HIST
        WHERE 1=1
        +Option2
        +Option3
		+Option1
        ORDER BY INSDTTM DESC
    </MainSQL>
    <Option>
        <Option1 type="IF" condition="not_equal" key="TOPNUMBER" dataType="NUM" default="0">
			<![CDATA[AND ROWNUM < @TOPNUMBER]]>
        </Option1>
        <Option2 type="IF" condition="not_equal" key="DURABLE_ID" dataType="string" default="">
            AND CSTID IN (@DURABLE_ID) 
        </Option2>
        <Option3 type="IF" condition="not_equal" key="PORT_ID" dataType="string" default="">
			AND PORT_ID LIKE @PORT_ID   -- 바인딩 안됨
		</Option3>
    </Option>
    <AdditionalVariable/>
    <Coloring/>
    <EventValues/>
</SearchCarrierEventAbbreviatedRecord>