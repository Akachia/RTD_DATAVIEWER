<?xml version="1.0" encoding="utf-8" ?>
<SearchTransportJobRecord>
    <MainSQL>
		SELECT
		CSTID
		, LOTID
		, FROM_EQPTID
		, FROM_PORT_ID
		, TO_EQPTID
		, TO_PORT_ID
		, CSTSTAT
		, RTD_RULE_ID
		, CMD_STAT_CODE
		, INSUSER
		, TRF_CLSS_CODE AS 반송주체
		, TRF_CODE
		, INSDTTM
		, UPDDTTM
		, (CASE WHEN (SYSDATE - INSDTTM) * 24 * 60 >= 60 THEN '1' ELSE 'O' END) AS CMDCHK
		FROM TB_MHS_TRF_CMD_HIST
		WHERE INSDTTM BETWEEN TO_DATE(@StartTime, 'YYYY-MM-DD') AND TO_DATE(@EndTime, 'YYYY-MM-DD HH24:MI:SS')

		+Option1
		+Option2  -- 바인딩 안됨
		+Option3  -- 바인딩 안됨
		+Option4
		+Option5  -- 바인딩 안됨
		+Option6  -- 바인딩 안됨
		ORDER BY INSDTTM DESC
	</MainSQL>
    <Option>
        <Option1 type="IF" condition="not_equal" key="DURABLE_ID" dataType="" default="">
            AND CSTID IN (@DURABLE_ID)
        </Option1>
        <Option2 type="IF" condition="not_equal" key="FROM_PORT_ID" dataType="" default="">
            AND FROM_PORT_ID LIKE @FROM_PORT_ID
        </Option2>
        <Option3 type="IF" condition="not_equal" key="TO_PORT_ID" dataType="" default="">
            AND TO_PORT_ID LIKE @TO_PORT_ID
        </Option3>
        <Option4 type="IF" condition="not_equal" key="DURABLE_STATUS_CODE" dataType="" default="">
            AND CSTSTAT = @DURABLE_STATUS_CODE
        </Option4>
        <Option5 type="IF" condition="not_equal" key="CMD_STAT_CODE" dataType="" default="">
            AND CMD_STAT_CODE = @CMD_STAT_CODE
        </Option5>
        <Option6 type="IF" condition="not_equal" key="RTD_RULE_ID" dataType="" default="">
            AND RTD_RULE_ID = @RTD_RULE_ID
        </Option6>
        <Option7 type="VAR" condition="" key="StartTime" dataType="DateTime" default=""/>
        <Option8 type="VAR" condition="" key="EndTime" dataType="DateTime" default=""/>
    </Option>
    <AdditionalVariable>
    </AdditionalVariable>
    <Coloring>
        <CSTSTAT Red="255" Green="155" Blue="155" IsColoringColumn="false" IsTimeCompare="false">
            <U Red="164" Green="201" Blue="255" IsColoringRow="false"/>
            <E Red="228" Green="239" Blue="255" IsColoringRow="false"/>
        </CSTSTAT>
    </Coloring>
    <EventValues/>
    <ColumnCount/>
</SearchTransportJobRecord>