<?xml version="1.0" encoding="utf-8" ?>
<SearchPortStateRecord>
    <MainSQL>
        select
        PORT_ID
        , PORT_STATE_CODE AS 상태
        , TRANSACTION_DATE
        , TRANSACTION_CODE
        from TB_EGN_PORT_HIST
        where PORT_ID like @PORT_ID  -- option1
        ORDER BY TRANSACTION_DATE desc
        +Option2
    </MainSQL>
    <Option>
        <Option1 type="VAR" condition="not_equal" key="PORT_ID" dataType="string" default=""/>
        <Option2 type="IF" condition="not_equal" key="TOPNUMBER" dataType="int" default="0">
            FETCH FIRST @TOPNUMBER ROWS ONLY
        </Option2>
    </Option>
    <AdditionalVariable/>
    <Coloring/>
    <EventValues/>
</SearchPortStateRecord>